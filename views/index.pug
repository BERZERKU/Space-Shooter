extends default

block content
  div.container
    each enemy in enemiesCategory
      div.enemyCard
        div.enemyCard-image(style=`background: url('/img/enemy.png') top center
        no-repeat; background-size: cover;`)
        h2.enemy-name
          | #{enemy.name}
        a.button(href=`/enemy?id=${enemy.id}`)
          | View Enemy